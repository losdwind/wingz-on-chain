{
  "name": "wingz-on-chain",
  "main": "index.js",
  "version": "1.0.0",
  "scripts": {
    "dev": "expo start -c --web",
    "dev:ios": "expo start -c --ios",
    "dev:android": "expo start -c --android",
    "android": "expo start -c --android",
    "ios": "expo start -c --ios",
    "web": "expo start -c --web",
    "clean": "rm -rf .expo node_modules",
    "postinstall": "npx tailwindcss -i ./global.css -o ./node_modules/.cache/nativewind/global.css",
    "lint": "expo lint",
    "prepare": "husky",
    "commit": "git-cz"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/native": "^6.0.2",
    "@reduxjs/toolkit": "^2.2.7",
    "@rn-primitives/avatar": "~1.0.4",
    "@rn-primitives/portal": "~1.0.3",
    "@rn-primitives/progress": "~1.0.3",
    "@rn-primitives/slot": "~1.0.3",
    "@rn-primitives/tooltip": "~1.0.3",
    "@rn-primitives/types": "~1.0.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "expo": "^51.0.24",
    "expo-linking": "~6.3.1",
    "expo-location": "~17.0.1",
    "expo-navigation-bar": "~3.0.7",
    "expo-router": "~3.5.20",
    "expo-splash-screen": "~0.27.5",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.7",
    "fast-text-encoding": "^1.0.6",
    "lucide-react-native": "^0.378.0",
    "nativewind": "^4.0.33",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.3",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-maps": "1.14.0",
    "react-native-maps-routes": "^1.0.3",
    "react-native-reanimated": "~3.10.1",
    "react-native-reanimated-carousel": "^3.5.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "~3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "~0.19.6",
    "react-redux": "^9.1.2",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "3.3.5",
    "tailwindcss-animate": "^1.0.7",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@commitlint/cli": "19.2.0",
    "@commitlint/config-conventional": "19.1.0",
    "@faker-js/faker": "^9.0.0",
    "@types/react": "~18.2.79",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.0",
    "eslint-config-expo": "^7.1.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "msw": "^2.4.5",
    "prettier": "^3.3.3",
    "typescript": "^5.3.3"
  },
  "private": true,
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint --config .eslintrc.js"
    ],
    "*.{ts,tsx,js,json,html,yml,css,less,scss,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint --config .commitlintrc.js -E HUSKY_GIT_PARAMS"
    }
  }
}
